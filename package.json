{
  "name": "tmp-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "VITEST_MIN_THREADS=1 VITEST_MAX_THREADS=1 vitest run",
    "test:mocked-copilot": "MOCK_COPILOT=1 VITEST_MIN_THREADS=1 VITEST_MAX_THREADS=1 vitest run tests/copilot",
    "test:openai": "RUN_OPENAI_TESTS=1 VITEST_MIN_THREADS=1 VITEST_MAX_THREADS=1 vitest run tests/integration/copilotApi.integration.test.ts tests/integration/mcpTools.integration.test.ts",
    "test:contracts:gen": "RUN_OPENAI_TESTS=1 VITEST_MIN_THREADS=1 VITEST_MAX_THREADS=1 vitest run tests/integration/contractGeneration.test.ts",
    "test:integration": "DOTENV_CONFIG_PATH=env/test.env RUN_OPENAI_TESTS=1 VITEST_MIN_THREADS=1 VITEST_MAX_THREADS=1 vitest run tests/integration",
    "seed": "npx prisma db seed",
    "seed:prod": "SEED_DB_TARGET=prod npx prisma db seed",
    "seed:test": "SEED_DB_TARGET=test DOTENV_CONFIG_PATH=env/test.env npx prisma db seed",
    "seed:demo": "ts-node --compiler-options '{\"module\":\"CommonJS\",\"moduleResolution\":\"node\"}' prisma/seedDemoData.ts",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f app",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d postgres",
    "db:test:up": "docker compose up -d postgres-test",
    "db:test:down": "docker compose stop postgres-test",
    "db:test:reset": "docker compose rm -sf postgres-test && docker compose up -d postgres-test"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.553.0",
    "mammoth": "^1.11.0",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "pdf-parse": "^1.1.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^1.6.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\",\"moduleResolution\":\"node\"} prisma/seed.ts"
  }
}
